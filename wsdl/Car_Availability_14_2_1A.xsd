<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/RCARRQ_14_2_1A" xmlns="http://xml.amadeus.com/RCARRQ_14_2_1A" elementFormDefault="qualified">
  <xs:element name="Car_Availability">
    <xs:annotation>
      <xs:documentation xml:lang="en">This message is used to request a car availability</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="scrollingInfo" minOccurs="0" type="ActionDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Scrolling information</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="language" minOccurs="0" maxOccurs="5" type="UserPreferencesType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Language requetsed by subscriber</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="carProviderIndicator" type="StatusType_79177S">
          <xs:annotation>
            <xs:documentation xml:lang="en">This flag selects if the availability involves: - the exhaustive list of car providers defined in elements providerSpecificOptions.  or - ALL amadeus car providers. The providers with specific options are defined in elements providerSpecificOptions.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="multimediaIndicator" minOccurs="0" type="StatusType_79177S">
          <xs:annotation>
            <xs:documentation xml:lang="en">use this field to request multimedia content to be returned in the availablity reply.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="multiMediaContent" minOccurs="0" maxOccurs="2">
          <xs:annotation>
            <xs:documentation xml:lang="en">if the multimedia content is requested (see multimediaIndicator) use this field to indicate the image and logo sizes</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="picturesType" type="ActionIdentificationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">these settings apply for  - the company logo or - the vehicle image</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pictureSize" maxOccurs="2" type="SelectionDetailsTypeI_79188S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">The sizes of the pictures to be retrieved. For each of these sizes, an appropriate picture URL is returned in the response.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="driverInfo" minOccurs="0" type="PassengerDocumentDetailsType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Driver information :  -Age   -Issue date of the drivers license</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="pickupDropoffInfo" minOccurs="0" maxOccurs="5">
          <xs:annotation>
            <xs:documentation xml:lang="en">Use this group to pass a list of "pickup and dropoff location details with dates". These are generic to all car providers. But can be overriden for specific providers (see providerSpecificOptions). It can be  - Airport Group (up to 5 occurences) - City Group (only one occurence) - Geocodes Group (only one occurence)</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="pickupDropoffTimes" type="StructuredPeriodInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Car location periode (from/to dates and hours).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="pickupDropoffInfo" maxOccurs="2">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Use this group to pass the Pickup and Dropoff location details. These are generic to all car providers.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="locationType" type="ActionIdentificationTypeI_79189S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">use this field to indicate if it is a pickup or a dropoff location.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="pointOfReference" minOccurs="0" type="PorInstanceDataType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Point of reference - search by POR category -  attraction , landmark  etc</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="iataAirportLocations" minOccurs="0" type="PlaceLocationIdentificationTypeU_79190S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">IATA airport code Remark: for city code use  element iataAirporCitytLocations</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="iataCitytLocations" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">IATA city code with Vicinity Code</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="locationServices" type="SelectionDetailsType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">use this field to indicate the vicinity code and to sort out the location against proposed services: . Car Delivery . Car Collection . Out-of-office Hour</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="iataAirportCityLocationCode" type="PlaceLocationIdentificationTypeU_79190S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Airport or City code</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="locationQuantity" minOccurs="0" type="ActionDetailsTypeI_79191S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Location Quantity</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="locationGeocodeInfo" minOccurs="0" type="GeneralGeographicalInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Geo-coding information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="providerSpecificOptions" minOccurs="0" maxOccurs="99">
          <xs:annotation>
            <xs:documentation xml:lang="en">Specific options for cars providers, which overrides the generic options.  Remark: If element carProviderIndicator is set to yes, the present list : - is an Exhaustive list of cars providers to be involved in the availability. otherwise it - represents cars providers specificities to override generic options.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="companyDetails" type="CompanyInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Car provider company code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="loyaltyNumbersList" minOccurs="0" maxOccurs="20">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Loyalty Numbers List</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="discountNumbers" type="ConsumerReferenceInformationTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Specific List of discount numbers to be applied together for this specific provider. If this list is specified, the generic list of discount numbers does not apply for this provider.  This discount numbers can be a - customer Number (ID) and/or  - Corporate Discount number (CD)         and/or - Promotional Code (PC) and/or  - an Airline Frequent Traveller Number (FT) - Rate Code and/or - Inbound Tour (IT)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="listIdentifier" minOccurs="0" type="ReferenceInfoType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Identifies the list of loyalty numbers.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="pickupDropoffInfos" minOccurs="0" maxOccurs="5">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Specific List of "pickup and dropoff location details with dates" to be applied together for this specific provider.  Warning: if this list is specified, the generic lists of "pickup and dropoff location details with dates" do not apply for this provider.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="pickupDropoffTimes" type="StructuredPeriodInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Car location periode (from/to dates and hours)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="pickupDropoffLocations" maxOccurs="2" type="PlaceLocationIdentificationTypeU">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">. IATA airport code . Amadeus Location code, or . Company Location code</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="bookingSource" minOccurs="0" type="AdditionalBusinessSourceInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Specific booking source to be applied for this specific provider.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="rateClass" type="AttributeType">
          <xs:annotation>
            <xs:documentation xml:lang="en">use this fied to request rates which are corporate or leisure or both.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="rateInformation" minOccurs="0" type="AttributeType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Can specify if full information of the rates should be returned (incl: policy , special equipment , policy link) or just the basic rate info</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="bookingSource" minOccurs="0" type="AdditionalBusinessSourceInformationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">The booking source</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="loyaltyNumbersList" minOccurs="0" maxOccurs="20">
          <xs:annotation>
            <xs:documentation xml:lang="en">Loyalty Numbers List</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="discountNumbers" type="ConsumerReferenceInformationTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Generic List of discount numbers to be applied together. These are generic to all car providers.  Warning: if a list of discounts numbers is entered for a specific provider, the present generic list of discount numbers does not apply for this provider.  This discount numbers can be a - an Airline Frequent Traveller Number (FT) and/or - Rate Code and/or - Inbound Tour (IT)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="listIdentifier" minOccurs="0" type="ReferenceInfoType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Identifies the list of Loyalty numbers.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="rateFilterOptions" minOccurs="0" type="StatusType_79177S">
          <xs:annotation>
            <xs:documentation xml:lang="en">by default, only rates with an estimated price and which are available are returned.  For less restrictions on the type of rates to be returned, please use this segment.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="rateAvailabilityOption" minOccurs="0" type="SelectionDetailsType_79176S">
          <xs:annotation>
            <xs:documentation xml:lang="en">Segment added to carry the FIR option (leisure option).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="rateinfo" minOccurs="0" type="TariffInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Rate requested: Rate category, Rate plan</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="vehicleTypeInfo" minOccurs="0" type="VehicleSelectionType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Information about the requested vehicle type.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="computeMarkups" type="ActionIdentificationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">compute mark-ups</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ruleReference" minOccurs="0" type="TicketingPriceSchemeType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Reference of the computation rule to be used for mark-up calculation. If empty, the default rule is applied.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="clientProfileOptions" minOccurs="0" type="StatusType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Parameters passed by the client that define how the client would like to use the CASA message.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="sortingRule" type="ActionIdentificationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Sorting rule to be applied on the returned rates</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="cacheOptions" minOccurs="0" maxOccurs="3" type="QuantityType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Cache parameters  enabling to specify : - the maximum response time  - the level of accuracy</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="filteringOptions" minOccurs="0" maxOccurs="10">
          <xs:annotation>
            <xs:documentation xml:lang="en">Use this field to add specific filtering option</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="matchType" type="StatusType_79177S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Return rates match/do not match the specified criterias</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="filterType" type="ActionIdentificationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Request a specific filter: M MultiMedia content filter O Vehicle Make Model filter P Price filter V Vehicle type filter</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="vehicleTypeInfo" minOccurs="0" type="VehicleSelectionType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">-------- NOT YET IMPLEMENTED ------ Filter on a specific  vehicle type</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="tariffInformation" minOccurs="0" maxOccurs="2" type="TariffInformationTypeI_79198S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">-------- NOT YET IMPLEMENTED --------</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ActionDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the scrolling information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="scrollingInformation" minOccurs="0" type="ReferenceTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Numbers of rows and reference information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ActionDetailsTypeI_79191S">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the action that should be taken on a selected reference number.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="numberOfItemsDetails" type="ProcessingInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">no comment</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ActionIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="actionRequestCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">LEI for leisure BMC for bypass mark-up computation LAB for leisure and bypass mark-up</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ActionIdentificationTypeI_79189S">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="locationType" type="AMA_EDICodesetType_Length1to3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Pickup or Dropoff Location</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ActionIdentificationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide details of action required or taken, the flights to which this action refers, and reasons for action taken.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="actionRequestCode" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">provides type of required action, coded.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalBusinessSourceInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify ownership and creator of a PNR</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="originatorDetails" type="OriginatorIdentificationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">ORIGINATOR DETAILS</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AreaType">
    <xs:annotation>
      <xs:documentation xml:lang="en">AREA</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="countryCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Country code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="stateCode" minOccurs="0" type="AlphaString_Length2To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">State code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="iataCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">IATA code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AssociatedChargesInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Identification of the additional charges that are associated to a product or service.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Mileage option</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to have tag value without code list for tag</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="criteriaSetType" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicators</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BusinessIdType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Contains the POR category and the POR business id when applicable</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="categoryCode" type="AlphaString_Length3To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Category the POR belongs to- can be : APT    AIRPORT                    ATT    ATTRACTION                 BST    BUS STATION                BUS    BUSINESS LOCATION          CAR    CAR RENTAL LOCATION        CNV    CONVENTION CENTER          CTY    CITY                       EDU    EDUCATIONAL                EVT    EVENT                      GRD    GROUND TRANSPORT           HEL    HELIPORT                   HLE    HOTEL LEISURE              HTL    HOTEL                      MIL    MILITARY BASES             PAR    PARK / RECREATIONAL AREA   PRT    PORT                       RAI    RAIL STATION               RST    RESORTS / SKI AREA   SHO    SHOPPING CENTER        SPO    SPORTS                 TRA    TRANSPORTATION POINTS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="foreignKey" minOccurs="0" type="AlphaNumericString_Length1To10">
        <xs:annotation>
          <xs:documentation xml:lang="en">POR business id</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">This segment is used to convey the non-air company information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="companyCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">This data element is used to convey the company code of a non-air company</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConsumerReferenceIdentificationTypeU">
    <xs:sequence>
      <xs:element name="referenceQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">RC, IT, FT</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="referenceNumber" minOccurs="0" type="AlphaNumericString_Length1To35">
        <xs:annotation>
          <xs:documentation xml:lang="en">value of the   Inclusive number Frequent Traveler number Promotional Code Rate Code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConsumerReferenceInformationTypeU">
    <xs:annotation>
      <xs:documentation xml:lang="en">This segment is used for different customer reference numbers including airline frequent traveller numbers if the communication is not between airlines</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="customerReferenceInfo" maxOccurs="20" type="ConsumerReferenceIdentificationTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">CUSTOMER REFERENCE INFORMATION</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DocumentDetailsType">
    <xs:sequence>
      <xs:element name="type" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Document type: PT for passport, VS for visa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="number" minOccurs="0" type="AlphaNumericString_Length1To20">
        <xs:annotation>
          <xs:documentation xml:lang="en">Passport number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="countryOfIssue" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Country where the document has been issued.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="expiryDate" minOccurs="0" type="AlphaNumericString_Length1To8">
        <xs:annotation>
          <xs:documentation xml:lang="en">Expiry date of the document. YYYYMMDD</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="issueDate" minOccurs="0" type="AlphaNumericString_Length1To8">
        <xs:annotation>
          <xs:documentation xml:lang="en">Date of issue of the document. YYYYMMDD</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GeneralGeographicalInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey car geo-coding information</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="position" type="GlobalPositioningSystemType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Longitude and  latitude of a point</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="distance" minOccurs="0" type="MeasurementsBatchTypeU">
        <xs:annotation>
          <xs:documentation xml:lang="en">Distance - radius around the point</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locationServices" minOccurs="0" type="SelectionDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Use this field to filter out the location against proposed services: . Car Delivery . Car Collection . Out-of-office Hour</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="locationQuantity" minOccurs="0" type="ActionDetailsTypeI_79191S">
        <xs:annotation>
          <xs:documentation xml:lang="en">-------- NOT YET IMPLEMENTED -------- LocationQuantity</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GeoCodeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Structure defining a geographycal position by latitude and longitude</xs:documentation>
    </xs:annotation>
    <xs:sequence>
